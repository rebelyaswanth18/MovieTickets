<div class="heading">
<b><font color="#0099e6" size="+4">List Of Movie's</font></b>
</div>
  <style>
    body
        {
            width: 100%;
            height: 600px;
            background:url('https://64.media.tumblr.com/773652562244df07d5cb7a7fc67faed1/7aadea73dba0e977-8d/s250x400/a6f8f2ca4ec21bb63080b226ccfa46fa32b6f7d2.gifv') no-repeat center;
            background-size: cover;
        }
    .heading
    {
      text-align: center;
      font-size: large;
      font-family: 'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif;
      font-weight: bolder;
    }

    .list
    {
      font-family: Verdana, Geneva, Tahoma, sans-serif;
      font-weight: 700;
    }
  </style>
  <div class = "list">
  <font color="#ff794d"><ul class="movies">
  <% for(var i=0; i<movies.length; i++) {%>
    <br>
    <li class="movies">
     <i><font size ="+2" color="#ff794d"><b><span><%= " No: " +movies[i].movieno+"."%></span>
      <br><span><%=" Name: " + movies[i].moviename%></span>
      <br><span><%=" Language: " + movies[i].movielang%></span>
      <br><span><%=" IMDB Rating: " + movies[i].movieimdb%></span></b></font></i>
    </li>
  <% } %>
</ul></font></div>
